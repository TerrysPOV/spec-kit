{
  "name": "spec-kit-orchestrator-monorepo",
  "private": true,
  "packageManager": "npm@10.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "turbo": "^2.0.0"
  },
  "scripts": {
    "dev:resume": "turbo run dev --filter @app/ai-resume-builder",
    "build:resume": "turbo run build --filter @app/ai-resume-builder",
    "lint:resume": "turbo run lint --filter @app/ai-resume-builder",
    "test:resume": "turbo run test --filter @app/ai-resume-builder"
  }
}